{
  "name": "electronjs-simple-boilerplate",
  "productName": "electronjs-simple-boilerplate",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "set NODE_ENV=development && electron-forge start",
    "package": "set NODE_ENV=production electron-forge package",
    "make": "set NODE_ENV=production electron-forge make",
    "make-win": "set NODE_ENV=production electron-forge make --platform win32",
    "make-linux": "set NODE_ENV=production electron-forge make --platform linux",
    "make-macos": "set NODE_ENV=production electron-forge make --platform darwin",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@electron-forge/cli": "^7.3.1",
    "@electron-forge/maker-deb": "^7.3.1",
    "@electron-forge/maker-rpm": "^7.3.1",
    "@electron-forge/maker-squirrel": "^7.3.1",
    "@electron-forge/maker-zip": "^7.3.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.1",
    "@electron-forge/plugin-fuses": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.3.1",
    "@electron/fuses": "^1.7.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.11.0",
    "electron": "29.2.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.4"
  },
  "keywords": [],
  "author": "PMDev",
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "os-browserify": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
